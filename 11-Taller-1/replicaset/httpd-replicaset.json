{
    "apiVersion": "apps/v1",
    "kind": "ReplicaSet",
    "metadata": {
        "name": "httpd-replicaset"
    },
    "spec": {
        "replicas": 5,
        "selector": {
            "matchLabels": {
                "app": "httpd"
            }
        },
        "template": {
            "metadata": {
                "labels": {
                    "app": "httpd"
                }
            },
            "spec": {
                "containers": [
                    {
                        "name": "httpd",
                        "image": "httpd",
                        "ports": [
                            {
                                "containerPort": 80
                            }
                        ]
                    }
                ]
            }
        }
    }
}